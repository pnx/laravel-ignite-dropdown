{
    "name": "pnx/laravel-ignite-dropdown",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Henrik Hautakoski",
            "email": "henrik.hautakoski@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "livewire/livewire": "^2.10",
        "pnx/laravel-ignite": "^0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Ignite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "suggest": {
        "blade-ui-kit/blade-icons": "For adding icon to the dropdown element"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ignite\\DropdownServiceProvider"
            ]
        }
    }
}
